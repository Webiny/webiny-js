{
  "extends": "../../tsconfig",
  "references": [
    { "path": "../app" },
    { "path": "../app-page-builder" },
    { "path": "../validation" }
  ],
  "compilerOptions": {
    "paths": {
      "@webiny/app/*": ["../app/src/*"],

{
  "extends": "../../tsconfig",
  "exclude": [
    "node_modules",
    "./dist",
    "**/__tests__/**",
    "../storybook-utils",
    "../form",
    "../validation"
  ],
  "references": ["../form", "../storybook-utils", "../validation"],
  "compilerOptions": {
    "paths": {
      "@webiny/form/*": ["../form/*"],
      "@webiny/form": ["../form"],
      "@webiny/storybook-utils/*": ["../storybook-utils/*"],
      "@webiny/storybook-utils": ["../storybook-utils"],
      "@webiny/validation/*": ["../validation/*"],
      "@webiny/validation": ["../validation"]
    }
  }
}

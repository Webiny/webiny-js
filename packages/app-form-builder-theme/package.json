{
  "name": "@webiny/app-form-builder-theme",
  "version": "4.0.0-beta.13",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@webiny/app-form-builder": "^4.0.0-beta.13",
    "@webiny/app-i18n": "^4.0.0-beta.13",
    "@webiny/form": "^4.0.0-beta.13",
    "@webiny/validation": "^4.0.0-beta.13"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "rimraf ./dist *.tsbuildinfo && babel src -d dist --source-maps --copy-files --extensions \".ts,.tsx\"",
    "watch": "babel src -d dist --source-maps --copy-files --extensions \".ts,.tsx\" --watch",
    "postbuild": "cp package.json LICENSE README.md dist/ && tsc -p tsconfig.build.json"
  }
}

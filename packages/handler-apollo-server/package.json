    "@webiny/handler": "^0.1.0",
    "@webiny/plugins": "^3.0.1",
    "@webiny/graphql": "^1.0.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "graphql-type-json": "^0.3.0",
    "graphql-type-long": "^0.1.1",
    "boolean": "^3.0.1",

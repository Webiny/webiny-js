{
  "extends": "../../tsconfig",
  "references": [
    {
      "path": "../db-dynamodb"
    },
    {
      "path": "../error"
    },
    {
      "path": "../api-i18n"
    },
    {
      "path": "../handler-db"
    }
  ],
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"],
      "@webiny/api-i18n/*": ["../api-i18n/src/*"],

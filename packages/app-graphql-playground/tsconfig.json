  "references": [
    "../app",
    "../app-admin",
    "../app-i18n",
    "../app-security",
    "../plugins",
    "../react-router",
    "../ui"
  ],
  "compilerOptions": {
    "paths": {
      "@webiny/app/*": ["../app/*"],

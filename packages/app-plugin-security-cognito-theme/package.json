  "license": "MIT",
  "dependencies": {
    "@emotion/styled": "^10.0.17",
    "@webiny/app": "^3.0.6",
    "@webiny/app-i18n": "^3.1.3",
    "@webiny/app-plugin-security-cognito": "^3.0.8",
    "@webiny/app-security": "^3.1.3",
    "@webiny/form": "^3.0.2",
    "@webiny/ui": "^3.2.1",
    "@webiny/validation": "^3.0.1",
    "classnames": "^2.2.6",
    "emotion": "^10.0.17",
    "lodash": "^4.17.11"
  },
  "peerDependencies": {
    "react": "^16.8.6"

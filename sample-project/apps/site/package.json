{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@webiny/app": "^3",
    "@webiny/app-cookie-policy": "^3",
    "@webiny/app-form-builder": "^2",
    "@webiny/app-google-tag-manager": "^3",
    "@webiny/app-i18n": "^3",
    "@webiny/app-mailchimp": "^3",
    "@webiny/app-page-builder": "^3",
    "@webiny/app-page-builder-theme": "^2",
    "@webiny/app-typeform": "^3",
    "@webiny/plugins": "^3",
    "@webiny/react-router": "^2",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-link-batch-http": "^1.2.3",
    "apollo-link-http": "^1.5.9",
    "core-js": "^3.2.1",
    "cross-fetch": "^3.0.2",
    "env-cmd": "^9.0.3",
    "emotion": "^10.0.27",
    "isutf8": "^2.1.0",
    "react-apollo": "^3.1.0",
    "react-app-env": "^1.2.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.5",
    "react-router-dom": "^5.0.0",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@apollo/react-ssr": "^3.0.1",
    "apollo-link-error": "^1.1.11",
    "babel-loader": "^8.0.6",
    "file-loader": "^4.2.0",
    "mime-types": "^2.1.24",
    "node-sass": "^4.9.3",
    "null-loader": "^1.0.0",
    "raw-loader": "^2.0.0",
    "react-scripts": "^3.3.0",
    "source-map-explorer": "^1.6.0",
    "webpackbar": "^4.0.0"
  },
  "peerDependencies": {
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.1",
    "react": "^16.4.0",
    "react-dom": "^16.6.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build:dev":  "env-cmd -r .env.json -e default,dev webiny run build",
    "build:prod": "env-cmd -r .env.json -e default,prod webiny run build",
    "build:ssr:dev":  "env-cmd -r .env.json -e default,dev webiny run build-ssr",
    "build:ssr:prod":  "env-cmd -r .env.json -e default,prod webiny run build-ssr",
    "start": "env-cmd -r .env.json -e default,dev webiny run start"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "svgo": {
    "plugins": {
      "removeViewBox": false
    }
  }
}
